{
  "name": "@blakeembrey/react-route",
  "version": "3.0.1",
  "description": "Path-to-RegExp with React Location",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist.es2015/index.js",
  "sideEffects": false,
  "files": [
    "dist/",
    "dist.es2015/"
  ],
  "scripts": {
    "prettier": "prettier --write",
    "lint": "tslint \"src/**/*\" --project tsconfig.json",
    "format": "npm run prettier -- \"{.,src/**}/*.{js,jsx,ts,tsx,json,md,yml,yaml}\"",
    "build": "rimraf dist/ dist.es2015/ && tsc && tsc -P tsconfig.es2015.json",
    "specs": "jest --coverage",
    "test": "npm run -s lint && npm run -s build && npm run -s specs && npm run -s size",
    "size": "size-limit",
    "example:simple": "webpack-dev-server --config examples/simple/webpack.js",
    "example:history": "webpack-dev-server --config examples/history/webpack.js",
    "example:hash": "webpack-dev-server --config examples/hash/webpack.js",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/blakeembrey/react-route.git"
  },
  "keywords": [
    "react",
    "route",
    "path-to-regexp",
    "router",
    "path",
    "match"
  ],
  "author": {
    "name": "Blake Embrey",
    "email": "hello@blakeembrey.com",
    "url": "http://blakeembrey.me"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/blakeembrey/react-route/issues"
  },
  "homepage": "https://github.com/blakeembrey/react-route",
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "2.3 kB"
    }
  ],
  "jest": {
    "roots": [
      "<rootDir>/src/"
    ],
    "transform": {
      "\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,yml,yaml}": [
      "npm run prettier",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@blakeembrey/react-location": "^1.4.0",
    "@size-limit/preset-small-lib": "^2.2.0",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "husky": "^3.0.2",
    "jest": "^24.1.0",
    "lint-staged": "^9.2.1",
    "prettier": "^1.14.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.1",
    "react-location": "^2.0.5",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.0.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.3.3"
  },
  "peerDependencies": {
    "react": "*",
    "@blakeembrey/react-location": "^1.4.0"
  },
  "dependencies": {
    "path-to-regexp": "^5.0.0"
  }
}
