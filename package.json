{
  "name": "@blakeembrey/react-route",
  "version": "3.0.0",
  "description": "Path-to-RegExp with React Location",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "prettier": "prettier --write",
    "lint": "tslint \"src/**/*\" --project tsconfig.json",
    "format": "npm run prettier -- \"src/**/*\"",
    "build": "rimraf dist && tsc",
    "specs": "jest --coverage",
    "test": "npm run -s lint && npm run -s build && npm run -s specs",
    "example:simple": "webpack-dev-server --config examples/simple/webpack.js",
    "example:history": "webpack-dev-server --config examples/history/webpack.js",
    "example:hash": "webpack-dev-server --config examples/hash/webpack.js",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/blakeembrey/react-route.git"
  },
  "keywords": [
    "react",
    "route",
    "path-to-regexp",
    "router",
    "path",
    "match"
  ],
  "author": {
    "name": "Blake Embrey",
    "email": "hello@blakeembrey.com",
    "url": "http://blakeembrey.me"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/blakeembrey/react-route/issues"
  },
  "homepage": "https://github.com/blakeembrey/react-route",
  "jest": {
    "roots": [
      "<rootDir>/src/"
    ],
    "transform": {
      "\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "npm run prettier",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@blakeembrey/react-location": "^1.2.0",
    "@types/jest": "^24.0.0",
    "@types/node": "^11.9.4",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "husky": "^1.2.0",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.14.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "rimraf": "^2.5.4",
    "ts-jest": "^23.1.4",
    "tslint": "^5.0.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.0",
    "typescript": "^3.3.3"
  },
  "peerDependencies": {
    "react": "*",
    "@blakeembrey/react-location": "^1.2.0"
  },
  "dependencies": {
    "path-to-regexp": "^3.0.0"
  }
}
